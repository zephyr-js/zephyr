{
  "name": "create-zephyr-app",
  "version": "0.2.2",
  "description": "Create Zephyr powered Express TS applications with one command",
  "author": {
    "name": "KaKeng Loh",
    "email": "kakengloh@gmail.com"
  },
  "homepage": "https://github.com/zephyr-js/zephyr",
  "repository": {
    "type": "git",
    "url": "https://github.com/zephyr-js/zephyr",
    "directory": "packages/create-zephyr-app"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "express",
    "zephyr",
    "zephyr.js"
  ],
  "bin": {
    "create-zephyr-app": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --fix src",
    "dev": "ncc build src/index.ts -w -o dist && cp -R src/templates dist/templates",
    "build": "ncc build src/index.ts -o dist --minify --no-cache --no-source-map-register && cp -R src/templates dist/templates"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.34.0",
    "@types/node": "^18.11.9",
    "tsconfig": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "cpy": "^7.3.0",
    "chalk": "^5.1.2",
    "cross-spawn": "^7.0.3",
    "@types/cross-spawn": "^6.0.2",
    "rimraf": "^3.0.2",
    "@types/rimraf": "^3.0.2",
    "validate-npm-package-name": "^5.0.0",
    "@types/validate-npm-package-name": "^4.0.0",
    "commander": "^9.4.1",
    "prompts": "^2.4.2",
    "update-check": "^1.5.4",
    "@types/prompts": "^2.4.1",
    "typescript": "^4.8.4"
  }
}