{
  "name": "create-zephyr-app",
  "version": "0.2.3",
  "description": "Create Zephyr powered Express TS applications with one command",
  "author": {
    "name": "KaKeng Loh",
    "email": "kakengloh@gmail.com"
  },
  "homepage": "https://github.com/zephyr-js/zephyr",
  "repository": {
    "type": "git",
    "url": "https://github.com/zephyr-js/zephyr",
    "directory": "packages/create-zephyr-app"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "express",
    "zephyr",
    "zephyr.js"
  ],
  "bin": {
    "create-zephyr-app": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --fix src",
    "dev": "ncc build src/index.ts -w -o dist",
    "build": "ncc build src/index.ts -o dist --minify --no-cache --no-source-map-register"
  },
  "dependencies": {
    "@astrojs/cli-kit": "^0.1.5",
    "chalk": "^5.0.1",
    "comment-json": "^4.2.3",
    "execa": "^6.1.0",
    "giget": "^0.1.7",
    "kleur": "^4.1.4",
    "ora": "^6.1.0",
    "prompts": "^2.4.2",
    "strip-ansi": "^7.0.1",
    "which-pm-runs": "^1.1.0",
    "yargs-parser": "^21.0.1"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.34.0",
    "@types/node": "^18.11.9",
    "tsconfig": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "@types/prompts": "^2.0.14",
    "@types/which-pm-runs": "^1.0.0",
    "@types/yargs-parser": "^21.0.0"
  }
}